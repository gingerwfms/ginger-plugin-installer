{
    "name": "gingerwfms/ginger-plugin-installer",
    "type": "zend-module",
    "description": "Plugin Installer for the Ginger Workflow Management System",
    "license": "BSD-3-Clause",    
    "homepage": "http://www.codeliner.ws/",
    "type": "composer-installer",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "kontakt@codeliner.ws",
            "homepage": "http://www.codeliner.ws/"
        }
    ],
    "keywords": [
        "ginger",
        "wfms",
        "plugin",
        "installer"
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers" : "1.0.*"
    },
    "require-dev" : {
        "phpunit/phpunit": "3.7.*",
        "composer/composer": "1.0.*@dev",
        "gingerwfms/ginger-core" : "dev-master",
        "zendframework/zendframework" : "2.2.*",
        "malocher/cqrs-esb" : "1.*",
        "malocher/zf2-cqrs-module" : "1.*"
    },
    "autoload": {
        "psr-0": {
            "GingerPluginInstaller\\": "src",
            "GingerPluginInstallerTest\\" : "tests"
        }
    },
    "extra": {
        "class": "GingerPluginInstaller\\Composer\\GingerInstaller"
    },
    "minimum-stability" : "dev"
}
